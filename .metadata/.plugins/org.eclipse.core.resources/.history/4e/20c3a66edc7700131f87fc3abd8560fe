import android.view.LayoutInflater;
import android.view.View;
import android.view.ViewGroup;
import android.widget.ArrayAdapter;
import android.widget.TextView;
public class cutadapter extends counter{
	private ArrayList<counter> entriess;
    private Activity activity;
 
    public cutadapter(Context context, int textViewResourceId, ArrayList<counter> entries) {
        super(context, textViewResourceId, entriess);
        this.entriess = entries;
        this.activity = a;
    }
 
    public static class ViewHolder{
        public TextView item1;
        public TextView item2;
    }
 
    

}
